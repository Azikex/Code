package com.code;

public class BianLiMax {
    public static void main(String[]args){
        int [] arr={1,5,7,9,15,17,32 };
        int Max=maxGet(arr);
        System.out.println(Max);

    }
    public static int maxGet(int [] arr){
        int Max = arr[0];
        for (int i = 1; i < arr.length; i++) {
        if (arr[i]>Max){
            Max=arr[i];
        }
        }
        return Max;
    }
}
